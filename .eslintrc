{
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "classes": true,
    "defaultParams": true,
    "destructuring": true,
    "forOf": true,
    "jsx": true,
    "modules": true,
    "objectLiteralShorthandMethods": true,
    "objectLiteralShorthandProperties": true,
    "restParams": true,
    "spread": true,
    "superInFunctions": true,
    "templateStrings": true
  },
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "curly": [2, "multi-line"],
    "quotes": [2, "single"],
    "strict": [2, "never"],
    "eol-last": [0],
    "func-style": [2, "declaration", {allowArrowFunctions: true}],
    "new-cap": [0],
    "no-debugger": 1,
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "no-unused-vars": 1,
    "no-use-before-define": [2, "nofunc"],
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2
  },
  "plugins": [
    "react"
  ]
}
